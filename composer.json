    "keywords": ["command bus", "ddd"],
    "homepage": "http://github.com/Innmind/CommandBus",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/CommandBus/issues"
    },
    "require": {
        "php": "~7.0",
        "innmind/immutable": "~1.9",
        "psr/log": "^1.0",
        "ramsey/uuid": "^3.5",
        "innmind/reflection": "^1.1"
    },
    "autoload": {
